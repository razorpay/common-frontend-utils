{
  "name": "fe",
  "version": "1.0.0",
  "main": "rollup.config.js",
  "scripts": {
    "build-coverage": "nyc --require @babel/register --reporter=html --reporter=text npm run test",
    "coverage": "nyc --require @babel/register npm run test",
    "document": "node document.js",
    "lint-documentation": "documentation lint src --shallow",
    "start": "rollup -cw",
    "test:base": "mocha --require @babel/register --require jsdom-global/register",
    "test:singular": "npm run test:base",
    "test": "npm run test:base \"tests/**/*.js\"",
    "play:implicit": "rollup -cw --config play/implicit/play.implicit.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/razorpay/common-frontend-utils.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "autoprefixer-stylus": "^0.14.0",
    "babel-eslint": "^10.0.2",
    "chokidar": "^3.0.2",
    "ecstatic": "^4.1.2",
    "eslint": "^6.2.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "fast-async": "^7.0.6",
    "glob": "^7.1.4",
    "http-proxy": "^1.17.0",
    "rollup": "^1.21.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-inject": "^3.0.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "stylus": "^0.54.7",
    "svelte": "^3.15.0",
    "uglify-js": "^3.6.0",
    "yargs-parser": "^13.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "documentation": "^12.1.1",
    "husky": "^3.0.4",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "bin": {
    "lintOutput": "scripts/lintOutput.js",
    "$": "bin/$",
    "serve": "bin/serve"
  }
}
