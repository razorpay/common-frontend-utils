{
  "name": "fe",
  "version": "1.0.0",
  "main": "rollup.config.js",
  "scripts": {
    "build-coverage": "nyc --require @babel/register --reporter=html --reporter=text npm run test",
    "coverage": "nyc --require @babel/register npm run test",
    "document": "node document.js",
    "lint-documentation": "documentation lint src --shallow",
    "start": "rollup -cw",
    "test:base": "mocha --require @babel/register --require jsdom-global/register",
    "test:singular": "npm run test:base",
    "test": "npm run test:base \"tests/**/*.js\"",
    "play:implicit": "rollup -cw --config play/implicit/play.implicit.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/razorpay/common-frontend-utils.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-block-scoped-functions": "^7.2.0",
    "@babel/plugin-transform-block-scoping": "^7.2.0",
    "@babel/plugin-transform-classes": "^7.2.2",
    "@babel/plugin-transform-computed-properties": "^7.2.0",
    "@babel/plugin-transform-destructuring": "^7.3.2",
    "@babel/plugin-transform-parameters": "^7.2.0",
    "@babel/plugin-transform-shorthand-properties": "^7.2.0",
    "@babel/plugin-transform-template-literals": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "autoprefixer-stylus": "^0.14.0",
    "babel-eslint": "^10.0.2",
    "chokidar": "^2.1.0",
    "ecstatic": "^3.3.0",
    "eslint": "^6.6.0",
    "fast-async": "^7.0.6",
    "glob": "^7.1.3",
    "http-proxy": "^1.17.0",
    "nodent-compiler": "^3.2.11",
    "rollup": "^1.21.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-inject": "^2.2.0",
    "rollup-plugin-svelte": "^5.0.1",
    "stylus": "^0.54.7",
    "svelte": "2.16.1",
    "uglify-js": "^3.4.9",
    "yargs-parser": "^13.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "documentation": "^11.0.1",
    "husky": "^1.3.1",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.10.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0",
    "rollup": "^0.66.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "bin": {
    "lintOutput": "scripts/lintOutput.js",
    "$": "bin/$",
    "serve": "bin/serve"
  }
}
